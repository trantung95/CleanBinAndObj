{
  "name": "clean-bin-obj",
  "displayName": "Clean Bin Obj - .NET Build Cleaner & Rebuild Tool",
  "description": "Instantly clean bin and obj folders in .NET C# F# VB projects. Delete build artifacts, remove temporary files, and rebuild solutions with one click. Supports .NET 6/7/8/9/10+ and .NET Framework. Perfect for Visual Studio Code .NET developers.",
  "version": "2.1.2",
  "publisher": "hybr8",
  "homepage": "https://github.com/trantung95/CleanBinAndObj#readme",
  "bugs": {
    "url": "https://github.com/trantung95/CleanBinAndObj/issues"
  },
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Programming Languages",
    "Linters",
    "Other"
  ],
  "tags": [
    "dotnet", "csharp", "fsharp", "vb.net", "cleanup", "clear", "cleaner", "clean-up", "clear-cache",
    "delete", "remove", "build", "rebuild", "msbuild", "artifacts", "cache", "temporary-files",
    "disk-space", "visual-studio", "productivity", "optimization", "development-tools", "project-cleanup"
  ],
  "icon": "icon.png",
  "galleryBanner": {
    "color": "#0078D7",
    "theme": "dark"
  },
  "qna": "marketplace",
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "cleanBinObj.clean",
        "title": "Clean Bin and Obj",
        "category": "Build"
      },
      {
        "command": "cleanBinObj.cleanWorkspace",
        "title": "Clean Bin and Obj in Entire Workspace",
        "category": "Build"
      },
      {
        "command": "cleanBinObj.cleanCurrentProject",
        "title": "Clean Bin and Obj in Current Project",
        "category": "Build"
      },
      {
        "command": "cleanBinObj.cleanAndRebuildWorkspace",
        "title": "Clean, Rebuild Entire Workspace",
        "category": "Build"
      },
      {
        "command": "cleanBinObj.cleanAndRebuildCurrentProject",
        "title": "Clean, Rebuild Current Project",
        "category": "Build"
      },
      {
        "command": "cleanBinObj.banhMi",
        "title": "ðŸ¥– Send me a BÃ¡nh MÃ¬",
        "category": "Clean Bin Obj"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "cleanBinObj.cleanCurrentProject",
          "group": "2_workspace@1",
          "when": "explorerResourceIsFolder || resourceLangId"
        }
      ],
      "editor/context": [
        {
          "command": "cleanBinObj.cleanCurrentProject",
          "group": "1_modification@1",
          "when": "editorTextFocus"
        }
      ],
      "commandPalette": [
        {
          "command": "cleanBinObj.clean"
        },
        {
          "command": "cleanBinObj.cleanWorkspace"
        },
        {
          "command": "cleanBinObj.cleanCurrentProject"
        },
        {
          "command": "cleanBinObj.cleanAndRebuildWorkspace"
        },
        {
          "command": "cleanBinObj.cleanAndRebuildCurrentProject"
        }
      ]
    },
    "configuration": {
      "title": "Clean Bin and Obj",
      "properties": {
        "cleanBinObj.targetSubdirectories": {
          "type": "array",
          "default": ["bin", "obj"],
          "description": "Subdirectories to clean in project folders",
          "items": {
            "type": "string"
          }
        },
        "cleanBinObj.projectPatterns": {
          "type": "array",
          "default": ["**/*.csproj", "**/*.fsproj", "**/*.vbproj", "**/*.sln"],
          "description": "Glob patterns to identify project files",
          "items": {
            "type": "string"
          }
        },
        "cleanBinObj.showOutputChannel": {
          "type": "boolean",
          "default": true,
          "description": "Show output channel when cleaning"
        }
      }
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.75.0",
    "@types/node": "16.x",
    "eslint": "^8.0.0"
  },
  "repository": {
